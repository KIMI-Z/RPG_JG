package jg.rpg.utils.config;

import java.io.File;

public class ConfigMgr {

	private static ConfigMgr inst;
	public static ConfigMgr getInstance(){
		if(inst == null){
			inst = new ConfigMgr();
		}
		return inst;
	}
	
	private ConfigMgr(){
		string path = System.getProperty("user.dir")+File.separator+"config"+File.separator+"config.xml";
		init("");
	}

	private void init(String path) {
		
		
	}
}

